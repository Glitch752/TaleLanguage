let TestClass = class {
    constructor(param) {
        let self = this;
        self.number = param;
    }

    printNumber() {
        print(this.number);
        print("\n");
    }

    addNumber(addend) {
        this.number = this.number + addend;
    }
};

let test = TestClass(5);
print(test.number);

