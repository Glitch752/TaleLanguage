let fibonacci = function(n) {
    if(n == 0) {
        return 0;
    }
    if(n == 1) {
        return 1;
    }

    return fibonacci(n - 1) + fibonacci(n - 2);
};

let main = function() {
    let i = 0;
    while(i < 20) {
        print(fibonacci(i));
        print("\n");
        i = i + 1;
    }
};

main();