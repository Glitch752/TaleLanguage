let Std = import('std');

export let Vec3 = class {
    constructor(x, y, z) {
        this.x = x;
        this.y = y;
        this.z = z;
    }
    print() {
        Std.print("Vec3(" + Std.string(this.x) + ", " + Std.string(this.y) + ", " + Std.string(this.z) + ") ");
    }
    add(v) {
        this.x = this.x + v.x;
        this.y = this.y + v.y;
        this.z = this.z + v.z;
    }
};

export let TestConstant = 10;